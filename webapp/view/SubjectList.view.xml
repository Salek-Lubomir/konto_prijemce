<mvc:View
	controllerName="ui5.kp.controller.SubjectList"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<List
		headerText="{i18n>subjectListTitle}"
		class="sapUiResponsiveMargin"
		width="auto"
		items="{invoice>/Invoices}">

		<items>
			<ObjectListItem
				title="{invoice>Quantity} x {invoice>ProductName}"
			/>
		</items>

	</List>

</mvc:View>
