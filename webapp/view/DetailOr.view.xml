<mvc:View
	controllerName="ui5.kp.controller.DetailOr"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:sfm="sap.ui.comp.smartform"
	xmlns:sfd="sap.ui.comp.smartfield"
	xmlns:sfb="sap.ui.comp.smartfilterbar"
	displayBlock="true"
>
	<Page id="idDetailOr" title="{i18n>detailOrPageTitle}">
		<content>
			<sfm:SmartForm id="smartFormBase">
                <sfm:Group id="groupBase" title="">
                    <sfm:GroupElement id="groupElementBase1">
                        <sfd:SmartField id="idIdnSbj" textLabel="Id subjektu" value="{Idnsbj}"></sfd:SmartField>
                    </sfm:GroupElement>
                    <sfm:GroupElement id="groupElementBase2">
                        <sfd:SmartField id="idVrzSbj" textLabel="Verze subjektu" value="{Vrzsbj}"></sfd:SmartField>
                    </sfm:GroupElement>
			    </sfm:Group>
            </sfm:SmartForm>
		</content>
	</Page>
</mvc:View>
